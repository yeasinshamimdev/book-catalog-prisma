generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id  String @id @default(uuid())
  name String
  password String
  role Role
  contactNo String
  address String
  profileImg String
}

enum Role {
  admin
  customer
}
